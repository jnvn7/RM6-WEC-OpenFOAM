Yfp     0;
vertices
(
    ( $Xfn  $Yfn $Zfn)
    ( $Xfp  $Yfn $Zfn)
    ( $Xfp  $Yfp $Zfn)
    ( $Xfn  $Yfp $Zfn)

    ( $Xfn  $Yfn $Zfp)
    ( $Xfp  $Yfn $Zfp)
    ( $Xfp  $Yfp $Zfp)
    ( $Xfn  $Yfp $Zfp)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($xCellsf $yCellsf $zCellsf) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    sides
    {
        type overset;
        faces
        (
        (0 3 2 1)
        (0 4 7 3)
        (1 2 6 5)
        (4 7 6 5)
        );
    }
    symFront
